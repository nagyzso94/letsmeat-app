<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_reviews"
    app:startDestination="@id/reviewFragment2">
    <fragment
        android:id="@+id/reviewAddFragment2"
        android:name="com.letsmeatapp.letsmeatapp.ui.review.ReviewAddFragment"
        android:label="fragment_review_add"
        tools:layout="@layout/fragment_review_add" >
        <argument
            android:name="currentRestaurant"
            app:argType="com.letsmeatapp.letsmeatapp.data.responses.Restaurant" />
        <action
            android:id="@+id/action_reviewAddFragment2_to_reviewFragment2"
            app:destination="@id/reviewFragment2" />
    </fragment>
    <fragment
        android:id="@+id/reviewFragment2"
        android:name="com.letsmeatapp.letsmeatapp.ui.review.ReviewFragment"
        android:label="fragment_review"
        tools:layout="@layout/fragment_review" >
        <action
            android:id="@+id/action_reviewFragment2_to_reviewAddFragment2"
            app:destination="@id/reviewAddFragment2" />
    </fragment>
</navigation>